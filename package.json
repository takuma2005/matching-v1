{
  "name": "tutor-matching-app",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "cross-env EXPO_PUBLIC_SKIP_AUTH=true expo start",
    "start:skip": "cross-env EXPO_PUBLIC_SKIP_AUTH=true expo start",
    "start:skip:clean": "cross-env EXPO_PUBLIC_SKIP_AUTH=true expo start -c",
    "android": "expo start --android",
    "android:skip": "cross-env EXPO_PUBLIC_SKIP_AUTH=true expo start --android",
    "android:skip:clean": "cross-env EXPO_PUBLIC_SKIP_AUTH=true expo start --android -c",
    "ios": "expo start --ios",
    "ios:skip": "cross-env EXPO_PUBLIC_SKIP_AUTH=true expo start --ios",
    "ios:skip:clean": "cross-env EXPO_PUBLIC_SKIP_AUTH=true expo start --ios -c",
    "web": "expo start --web",
    "web:skip": "cross-env EXPO_PUBLIC_SKIP_AUTH=true expo start --web",
    "web:skip:clean": "cross-env EXPO_PUBLIC_SKIP_AUTH=true expo start --web -c",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write .",
    "dev": "EXPO_NO_TELEMETRY=1 expo start",
    "prepare": "husky install"
  },
  "dependencies": {
    "@expo/metro-runtime": "~6.1.2",
    "@hookform/resolvers": "^5.2.1",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/blur": "^4.4.1",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-navigation/bottom-tabs": "^7.1.5",
    "@react-navigation/native": "^7.0.13",
    "@react-navigation/stack": "^7.1.1",
    "date-fns": "^4.1.0",
    "expo": "54.0.6",
    "expo-blur": "~15.0.7",
    "expo-font": "~14.0.8",
    "expo-linear-gradient": "~15.0.7",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.62.0",
    "react-native": "0.81.4",
    "react-native-calendars": "^1.1313.0",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-get-random-values": "^1.11.0",
    "react-native-modal-datetime-picker": "^18.0.0",
    "react-native-reanimated": "~4.1.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-uuid": "^2.0.3",
    "react-native-web": "^0.21.0",
    "react-native-worklets": "^0.5.1",
    "uuid": "^12.0.0",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.1.10",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "babel-jest": "^30.1.2",
    "babel-plugin-module-resolver": "^5.0.2",
    "cross-env": "^10.0.0",
    "eslint": "^8.57.1",
    "eslint-config-universe": "^12.0.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.1.6",
    "jest": "~29.7.0",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "react-test-renderer": "19.1.0",
    "typescript": "~5.9.2"
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "**/*.{json,md}": [
      "prettier --write"
    ]
  },
  "private": true
}
